{
  "name": "Canvas Hide Old Courses",
  "version": "1.0",
  "description": "Hide old course on Canvas",
  "manifest_version": 2,
  "permissions": ["tabs"],
  "content_scripts": [
    {
      "matches": ["https://*.instructure.com/courses"],
      "js": ["move_course.js"],
      "run_at": "document_start"
    }
  ]
}
